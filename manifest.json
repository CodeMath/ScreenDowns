{
  "name": "스샷 캡쳐 다운로드",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "크롬 브라우저 풀 스크린 캡쳐 다운로드",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [ "activeTab","downloads" ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
